TMP = _build

vpath %.cmi TMP
vpath %.cmo
all: graphToC.byte
	@echo -e "abc"

%.byte :
	ocamlbuild $@
%.cmo :
	ocamlbuild $@
%.cmi :
	ocamlbuild $@


%.cmi : %.ml | $(TMP)/
	ocamlc -I tmp -o $@ -c $<
# $(TMP)/%.cmi: %.ml | $(TMP)/
# 	ocamlc -c $<

$(TMP)/:
	mkdir -p $@

$(TMP)/graphToC.cmo: $(TMP)/typesgraphe.cmo $(TMP)/typesgraphe.cmi